{
  "name": "My_Project_Here",
  "version": "1.0.0",
  "private": true,
  "description": "Description of project here",
  "scripts": {
    "devcontainer:setup": "yarn reset && yarn docker:run-setup",
    "devcontainer:dev": "bash -c \". ../graphile-starter-app/.envvar && yarn docker:up:web && yarn docker:attach:dev yarn dev",
    "reset": "bash -c \"yarn docker:reset || true\"",
    "/* TOOLS */":"//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////",
    "docker:compose": "docker-compose --project-name graphile-starter_devcontainer -f docker-compose.yml",
    "docker:up:web":"yarn docker:compose up -d pg dev",
    "docker:attach": "yarn docker:compose exec",
    "docker:attach:dev": "yarn docker:attach dev",
    "docker:reset":"yarn docker:compose down && docker volume rm devcontainer_db-volume devcontainer_node_modules-volume",
    "docker:run-setup": "yarn docker:rebuild && yarn docker:up:web && yarn docker:compose exec dev yarn setup",
    "docker:rebuild":"yarn docker:compose build"
  },
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
